{
  "name": "website",
  "author": {
    "name": "Ye Shu",
    "email": "hello@shuye.dev",
    "url": "https://shuye.dev"
  },
  "description": "The Personal Website for Ye Shu",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --no-open",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "eslint . --ext=js,jsx,ts,tsx",
    "lint:fix": "eslint --fix . --ext=js,jsx,ts,tsx",
    "format": "prettier --check '**/*.{js,jsx,ts,tsx,html,css,md,mdx}'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,html,css,md,mdx}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@docusaurus/core": "2.2.0",
    "@docusaurus/preset-classic": "2.2.0",
    "hast-util-is-element": "1.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-masonry-css": "1.0.16",
    "react-photo-album": "2.0.0",
    "rehype-katex": "4.0.0",
    "remark-math": "3.0.1",
    "yet-another-react-lightbox": "2.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.2.0",
    "@tsconfig/docusaurus": "1.0.6",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.1",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "prettier": "2.8.3",
    "typescript": "4.9.4"
  },
  "resolutions": {
    "@types/react": "17.0.53"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{md,mdx,json,html,css}": [
      "prettier --write"
    ]
  }
}
