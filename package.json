{
  "name": "website",
  "author": {
    "name": "Ye Shu",
    "email": "hello@shuye.dev",
    "url": "https://shuye.dev"
  },
  "description": "The Personal Website for Ye Shu",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --no-open",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "eslint . --ext=js,jsx,ts,tsx",
    "lint:fix": "eslint --fix . --ext=js,jsx,ts,tsx",
    "format": "prettier --check '**/*.{js,jsx,ts,tsx,html,css,md,mdx}'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,html,css,md,mdx}'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{md,mdx,json,html,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "3.5.2",
    "@docusaurus/preset-classic": "3.5.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-masonry-css": "1.0.16",
    "react-photo-album": "2.4.1",
    "rehype-katex": "7.0.1",
    "remark-math": "6.0.0",
    "yet-another-react-lightbox": "3.21.6"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.5.2",
    "@docusaurus/tsconfig": "3.5.2",
    "@docusaurus/types": "3.5.2",
    "@typescript-eslint/eslint-plugin": "8.26.1",
    "@typescript-eslint/parser": "8.26.1",
    "eslint": "9.22.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-react": "7.36.1",
    "husky": "9.1.7",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "typescript": "5.6.2"
  },
  "resolutions": {
    "@types/react": "18.3.9"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "packageManager": "yarn@4.5.0"
}
