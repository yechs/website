{
  "name": "website",
  "author": {
    "name": "Ye Shu",
    "email": "chshu@protonmail.ch",
    "url": "https://shuye.dev"
  },
  "description": "The Personal Website for Ye Shu",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --no-open",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "eslint . --ext=js,jsx,ts,tsx",
    "lint:fix": "eslint --fix . --ext=js,jsx,ts,tsx",
    "format": "prettier --check '**/*.{js,jsx,ts,tsx,html,css,md,mdx}'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,html,css,md,mdx}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.0",
    "@docusaurus/preset-classic": "2.0.0-beta.0",
    "@mdx-js/react": "^1.6.21",
    "clsx": "^1.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-masonry-css": "^1.0.16"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-beta.0",
    "@tsconfig/docusaurus": "^1.0.2",
    "@types/react": "^17.0.5",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-eslint": "^10.0.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": ">=6",
    "lint-staged": ">=11.0.0",
    "prettier": "2.3.0",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{md,mdx,html,css}": [
      "prettier --write"
    ]
  }
}
